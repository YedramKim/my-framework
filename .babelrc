{
  "presets": [
		["@babel/preset-env", { "modules": false }],
		"@babel/preset-es2015",
		"@babel/preset-react"
  ],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-decorators",
		"@babel/plugin-proposal-object-rest-spread"
	],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", { "targets": { "node": "current" }}]
      ]
    }
  }
}