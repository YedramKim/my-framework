{
  "presets": [
		["env", {
				"modules": false,
				"targets": {
					"chrome": 52
				}
			}],
		"vue",
		"react"
  ],
	"plugins": [
		"transform-class-properties",
		"transform-object-rest-spread",
		"syntax-jsx",
		"jsx-v-model",
		"transform-vue-jsx",
		"jsx-event-modifiers"
	],
  "env": {
    "test": {
      "presets": [
        ["env", { "targets": { "node": "current" }}]
      ]
    }
  }
}